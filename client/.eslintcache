l, form_data, {\n        withCredentials: true,\n      });\n      if (data.msg) {\n        return this.SignInResposneAlert(status, data.msg);\n      }\n      return this.SignInResposneAlert(status, \"Account Successfully Logged In\");\n    } catch (error) {\n      const { data, status } = error.response;\n      this.SignInResposneAlert(status, data.msg);\n    }\n  }\n  render() {\n    return (\n      <div className=\"SignIn\">\n        <form className=\"Form\">\n          <h3 className=\"AuthForm__header\">{`${this.captilaziedRoleName} SignIn`}</h3>\n          <label>Username</label>\n          <input\n            type=\"text\"\n            placeholder=\"username\"\n            required\n            className=\"inputField\"\n            id=\"username\"\n            onChange={this.handleChange}\n          />\n\n          <label>Password</label>\n          <input\n            type=\"password\"\n            placeholder=\"Password\"\n            required\n            className=\"inputField\"\n            id=\"password\"\n            onChange={this.handleChange}\n          />\n          <button className=\"btn btn-signup\" onClick={this.SignInUser}>\n            SignIn\n          </button>\n        </form>\n        <ReactNotification className=\"Notifaction-card\" />\n      </div>\n    );\n  }\n}\n\nexport default withRouter(SignIn);\n","/home/junior/SIDE-PROJETS/ElegantDesign-ShipmentTrackingApp/client/src/Components/AdminDashBoard.tsx",[],"/home/junior/SIDE-PROJETS/ElegantDesign-ShipmentTrackingApp/client/src/Components/TakeNewOrder.tsx",[],"/home/junior/SIDE-PROJETS/ElegantDesign-ShipmentTrackingApp/client/src/store/Reducers/FetchAllOrderReducer/FetchAllOrderReducer.ts",[],"/home/junior/SIDE-PROJETS/ElegantDesign-ShipmentTrackingApp/client/src/store/Actions/FetchAllOrder/Actions.ts",[],"/home/junior/SIDE-PROJETS/ElegantDesign-ShipmentTrackingApp/client/src/Components/AllShipmentOrder.tsx",[],"/home/junior/SIDE-PROJETS/ElegantDesign-ShipmentTrackingApp/client/src/store